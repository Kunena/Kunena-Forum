    <license>GNU/GPLv3 or later</license>
    <description>Kunena media files.</description>
    <changelogurl>@kunenachangelog@</changelogurl>
    <fileset>
        <files target="media/kunena">
            <folder>attachments</folder>
            <folder>avatars</folder>
            <folder>category_images</folder>
            <folder>core</folder>
            <folder>email</folder>
            <folder>emoticons</folder>
            <folder>images</folder>
            <folder>labels</folder>
            <folder>ranks</folder>
            <folder>topic_icons</folder>
