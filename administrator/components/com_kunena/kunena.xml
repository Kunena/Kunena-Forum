<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE extension>
<extension type="component" version="2.5" method="upgrade">
	<name>com_kunena</name>
	<version>2.9.92-DEV</version>
	<versionname>Wanga</versionname>
	<creationDate>@kunenaversiondate@</creationDate>
	<author>Kunena Team</author>
	<authorEmail>kunena@kunena.org</authorEmail>
	<authorUrl>http://www.kunena.org</authorUrl>
	<copyright>(C) 2008 - 2013 Kunena Team. All rights reserved.</copyright>
	<license>GNU/GPLv3 or later</license>
	<description>COM_KUNENA_XML_DESCRIPTION</description>
	<scriptfile>install/install.script.php</scriptfile>
	<files>
		<filename>COPYRIGHT.php</filename>
	</files>
	<administration>
		<menu view="cpanel" img="components/com_kunena/media/icons/favicons/kunena-logo-white.png">COM_KUNENA</menu>
		<submenu>
			<menu view="config" img="components/com_kunena/media/icons/favicons/kunena-config.png">COM_KUNENA_CONFIGURATION</menu>
			<menu view="categories" img="components/com_kunena/media/icons/favicons/kunena-categories.png">COM_KUNENA_CATEGORY_MANAGER</menu>
			<menu view="users" img="components/com_kunena/media/icons/favicons/kunena-users.png">COM_KUNENA_USER_MANAGER</menu>
			<menu view="templates" img="components/com_kunena/media/icons/favicons/kunena-templates.png">COM_KUNENA_TEMPLATE_MANAGER</menu>
			<menu view="smilies" img="components/com_kunena/media/icons/favicons/kunena-smileys.png">COM_KUNENA_EMOTICON_MANAGER</menu>
			<menu view="ranks" img="components/com_kunena/media/icons/favicons/kunena-ranks.png">COM_KUNENA_RANK_MANAGER</menu>
			<menu view="trash" img="components/com_kunena/media/icons/favicons/kunena-trash.png">COM_KUNENA_TRASH_MANAGER</menu>
		</submenu>
		<files>
			<folder>install</folder>
			<folder>archive</folder>
			<folder>language</folder>
		</files>
	</administration>
	<updateservers>
		<server type="collection" priority="1" name="Kunena 3.0 Update Site">http://update.kunena.org/3.0</server>
	</updateservers>
</extension>
