<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE metadata>
<metadata>
	<layout title="Show Announcements">
		<message>
			<![CDATA[Show announcements]]>
		</message>
	</layout>
	<state>
		<name>Kunena Forum: Show Announcements</name>
		<description>Show announcements</description>
		<params>
			<param name="limit" type="text" default="10" label="Announcements in a Page" />
